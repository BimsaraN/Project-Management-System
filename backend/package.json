{
    "name": "sevalk",
    "version": "1.0.0",
    "description": "sevalk is the ultimate backend solution",
    "main": "index.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "sevalk"
    ],
    "license": "APACHE-2.0",
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "http-status": "^1.3.2",
        "md5": "^2.3.0",
        "multer": "^1.4.5-lts.1",
        "mysql": "^2.18.1",
        "uniqid": "^5.4.0"
    }
}
